{
  "name": "stylers-llc/laratask",
  "description": "Laravel Task Manager",
  "license": "MIT",
  "authors": [
    {
      "name": "Szilveszter Nagy",
      "email": "szilveszter.nagy@stylersonline.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.*",
    "stylers/taxonomy": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "orchestra/testbench": "^3.4",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "classmap": [
      "src/database"
    ],
    "psr-4": {
      "Stylers\\Laratask\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stylers\\Laratask\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stylers\\Laratask\\Providers\\LarataskServiceProvider"
      ]
    }
  }
}