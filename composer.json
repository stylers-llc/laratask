{
  "name": "stylers/laratask",
  "description": "Laravel Task Manager",
  "homepage": "https://github.com/stylers-llc/taxonomy",
  "keywords": [
        "stylers",
        "laravel",
        "task",
        "taxonomy"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Szilveszter Nagy",
      "email": "developer@stylersonline.com",
      "homepage": "http://stylers.hu"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5",
    "stylers/taxonomy": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "orchestra/testbench": "^3.4",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "classmap": [
      "src/database"
    ],
    "psr-4": {
      "Stylers\\Laratask\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stylers\\Laratask\\Tests\\": "tests",
      "Stylers\\Laratask\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stylers\\Laratask\\Providers\\LarataskServiceProvider"
      ]
    }
  }
}
